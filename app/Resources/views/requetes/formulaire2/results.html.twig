{# Résultats formulaire 2 #}

<fieldset>
  <legend>
    <h3>Résultats</h3>
  </legend>

  <ul class="nav nav-tabs" role="tablist" id="result-tabs">
    <li role="presentation" class="active disabled" id="recto-tab">
      <a
        href="#result-pane-recto"
        aria-controls="result-pane"
        role="tab"
        data-side='recto'
        data-toggle="tab">
        <strong>Référence
          <i class="fa fa-arrow-circle-right "></i>
          Méthodes
        </strong>
      </a>
    </li>
    <li role="presentation" class="disabled" id="verso-tab">
      <a
        href="#result-pane-verso"
        aria-controls="result-pane"
        role="tab"
        data-side='verso'
        data-toggle="tab">
        <strong>Méthodes
          <i class="fa fa-arrow-circle-right "></i>
          Référence</strong>
      </a>
    </li>
  </ul>
  <div class="tab-content">
    <div role="tabpanel" class="tab-pane fade in active" id="result-pane-recto">
      <div class="well well-sm">
        <div id="barplot-recto">
          <center>
            <i class="fa fa-bar-chart fa-4x" style="margin:5vh"></i><br>
          </center>
        </div>
        <hr>
        <div class="table-responsive" style='width:100%'>
          <table
            id="result-table-recto"
            class="table table-condensed table-hover table-striped"
            data-barplot="#barplot-recto"
            data-target="recto">
            <thead>
              <tr>
                <th>Méthode</th>
                <th>Jeu de données</th>
                <th>Match</th>
                <th>Split</th>
                <th>Lump</th>
                <th>Reshuffling</th>
              </tr>
            </thead>
            <tbody></tbody>
          </table>
        </div>
      </div>
    </div>
    <div role="tabpanel" class="tab-pane fade" id="result-pane-verso">
      <div class="well well-sm">
        <div id="barplot-verso">
          <center>
            <i class="fa fa-bar-chart fa-4x" style="margin:5vh"></i><br>
          </center>
        </div>
        <hr>
        <div class="table-responsive" style='width:100%'>
          <table
            id="result-table-verso"
            class="table table-condensed table-hover table-striped"
            data-barplot="#barplot-verso"
            data-target="verso">
            <thead>
              <tr>
                <th>Méthode</th>
                <th>Jeu de données</th>
                <th>Match</th>
                <th>Split</th>
                <th>Lump</th>
                <th>Reshuffling</th>
              </tr>
            </thead>
            <tbody></tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</fieldset>
