{% extends 'base.html.twig' %}

{% block stylesheets %}
  {{ parent() }}
  <!-- Switchery -->
  <link href="{{ asset('e3s/vendors/switchery/dist/switchery.min.css') }}" rel="stylesheet">
	<!-- Datatables -->
  <link href="{{ asset('e3s/vendors/datatables/datatables.min.css') }}" rel="stylesheet">
  <!-- Custom Theme Style -->
  <link href="{{ asset('e3s/css/queries.min.css') }}" rel="stylesheet">
	<!-- Custom Theme Style -->
  <link href="{{ asset('e3s/css/datatables.fa.css') }}" rel="stylesheet">
{% endblock %}

{% block headincl %}
  <!-- Plotly -->
  <script src="{{ asset('e3s/vendors/plotly.js/plotly-latest.min.js') }}"></script>
{% endblock %}

{% block titlepage %}
  <div class="clearfix"></div>
{% endblock %}

{% block action %}{% endblock %}

{% block body %}
	{% trans_default_domain 'requetes' %}
  <div class="clearfix"></div>
{% endblock %}

{% block link %}
  <div class="clearfix"></div>
{% endblock %}

{% block scripts %}
  {{ parent() }}
	<!-- Datatables -->
  <script src="{{ asset('e3s/vendors/datatables/datatables.min.js') }}"></script>
  <!-- Switchery -->
  <script src="{{ asset('e3s/vendors/switchery/dist/switchery.min.js') }}"></script>

  <!-- Custom Scripts -->
  <script src="{{ asset('e3s/js/queries.js') }}"></script>
  <script src="{{ asset('e3s/js/plots.js') }}"></script>
  <!-- Mustache templating engine -->
  <script src="{{ asset('e3s/vendors/mustache/mustache.js') }}"></script>
{% endblock %}
